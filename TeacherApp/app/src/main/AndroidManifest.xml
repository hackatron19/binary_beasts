    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Functions.Assets"></activity>
        <activity android:name=".Support.UploadAssets" />
        <activity android:name=".Support.MarksCourseList" />
        <activity android:name=".Functions.MarksSheet" />
